[theme]
name=noctalia-template
author=taktojakuba

# Core colors
accent={{colors.primary.default.hex}}
text={{colors.on_surface.default.hex}}
textDim={{colors.on_surface_variant.default.hex}}
textMuted={{colors.outline.default.hex}}

# General Colors
logo={{colors.outline.default.hex}}
header={{colors.primary.default.hex}}
footer={{colors.outline_variant.default.hex}}
help={{colors.secondary.default.hex}}
link={{colors.tertiary.default.hex}}
nowplaying={{colors.primary.default.hex}}

# Playlist View
playlist.rownum={{colors.on_surface_variant.default.hex}}
playlist.title={{colors.on_surface.default.hex}}
playlist.playing={{colors.primary.default.hex}}

# Track View
trackview.title={{colors.on_surface.default.hex}}
trackview.artist={{colors.primary.default.hex}}
trackview.album={{colors.secondary.default.hex}}
trackview.year={{colors.on_surface_variant.default.hex}}
trackview.time={{colors.tertiary.default.hex}}
trackview.visualizer={{colors.primary.default.hex}}
trackview.lyrics={{colors.on_surface.default.hex}}

# Library View
library.artist={{colors.on_surface_variant.default.hex}}
library.album={{colors.on_surface.default.hex}}
library.track={{colors.on_surface.default.hex}}
library.enqueued={{colors.secondary.default.hex}}
library.playing={{colors.primary.default.hex}}

# Search
search.label={{colors.on_surface_variant.default.hex}}
search.query={{colors.primary.default.hex}}
search.result={{colors.on_surface.default.hex}}
search.enqueued={{colors.secondary.default.hex}}
search.playing={{colors.primary.default.hex}}

# Progress
progress.filled={{colors.surface_container_highest.default.hex}}
progress.empty={{colors.surface.default.hex}}
progress.elapsed={{colors.tertiary.default.hex}}

# Status
status.info={{colors.on_surface.default.hex}}
status.warning={{colors.secondary.default.hex}}
status.error={{colors.tertiary.default.hex}}
status.success={{colors.primary.default.hex}}
